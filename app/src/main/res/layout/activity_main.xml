<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/bj"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.administrator.js.MainActivity">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bj"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <EditText
            android:id="@+id/textView"
            android:layout_width="370dp"
            android:layout_height="54dp"
            android:cursorVisible="false"
            android:background="@android:color/transparent"
            android:layout_row="7"
            android:layout_columnSpan="8"
            android:layout_column="0" />

        <Button
            android:id="@+id/clean"
            android:layout_width="177dp"
            android:layout_height="80dp"
            android:text="Clean"
            android:textColor="#0f9dda"
            android:background="@android:color/transparent"
            android:textSize="35dp"
            android:layout_row="27"
            android:layout_columnSpan="4"
            android:layout_column="0" />

        <Button
            android:id="@+id/back"
            android:layout_width="171dp"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="back"
            android:textColor="#0f9dda"
            android:textSize="35dp"
            android:layout_row="27"
            android:layout_columnSpan="4"
            android:layout_column="5" />

        <Button
            android:id="@+id/button1"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="1"
            android:textSize="35dp"
            android:textColor="#170d84"
            android:layout_row="28"
            android:layout_column="0"
            />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="2"
            android:textSize="35dp"
            android:textColor="#170d84"
            android:layout_row="28"
            android:layout_column="2" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="3"
            android:textSize="35dp"
            android:textColor="#170d84"
            android:layout_row="28"
            android:layout_column="5" />

        <Button
            android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="+"
            android:textColor="#0f9dda"
            android:textSize="35dp"
            android:layout_row="28"
            android:layout_column="6" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="4"
            android:textColor="#170d84"
            android:textSize="35dp"
            android:layout_row="29"
            android:layout_column="0" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="5"
            android:textColor="#170d84"
            android:textSize="35dp"
            android:layout_row="29"
            android:layout_column="2" />

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="6"
            android:textColor="#170d84"
            android:textSize="35dp"
            android:layout_row="29"
            android:layout_column="5" />

        <Button
            android:id="@+id/sub"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="-"
            android:textSize="35dp"
            android:textColor="#0f9dda"
            android:layout_row="29"
            android:layout_column="6" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="7"
            android:textColor="#170d84"
            android:textSize="35dp"
            android:layout_row="30"
            android:layout_column="0" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="8"
            android:textSize="35dp"
            android:textColor="#170d84"
            android:layout_row="30"
            android:layout_column="2" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="9"
            android:textSize="35dp"
            android:textColor="#170d84"
            android:layout_row="30"
            android:layout_column="5" />

        <Button
            android:id="@+id/mul"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="*"
            android:textColor="#0f9dda"
            android:textSize="35dp"
            android:layout_row="30"
            android:layout_column="6" />

        <Button
            android:id="@+id/button0"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="0"
            android:textSize="35dp"
            android:textColor="#170d84"
            android:layout_row="31"
            android:layout_column="0" />

        <Button
            android:id="@+id/point"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="."
            android:textSize="35dp"
            android:textColor="#0f9dda"
            android:layout_row="31"
            android:layout_column="2" />

        <Button
            android:id="@+id/equal"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="="
            android:textColor="#170d84"
            android:textSize="35dp"
            android:layout_row="31"
            android:layout_column="5" />

        <Button
            android:id="@+id/div"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:text="/"
            android:textColor="#0f9dda"
            android:textSize="35dp"
            android:layout_row="31"
            android:layout_column="6" />

    </GridLayout>
</RelativeLayout>
